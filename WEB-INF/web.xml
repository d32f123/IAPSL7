<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>AreaCheckServlet</display-name>
    <servlet>
        <servlet-name>AreaCheckServlet</servlet-name>
        <servlet-class>Lab7.AreaCheckServlet</servlet-class>
        <init-param>
            <param-name>a</param-name>
            <param-value>123</param-value>
        </init-param>
        <init-param>
            <param-name>b</param-name>
            <param-value>456</param-value>
        </init-param>
        <init-param>
            <param-name>c</param-name>
            <param-value>789</param-value>
        </init-param>
        <init-param>
            <param-name>list</param-name>
            <param-value></param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>AreaCheckServlet</servlet-name>
        <url-pattern>/checking</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ControllerServlet</servlet-name>
        <servlet-class>Lab7.ControllerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControllerServlet</servlet-name>
        <url-pattern>/controller</url-pattern>
    </servlet-mapping>
</web-app>
